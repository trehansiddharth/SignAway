040C: C4              swap  a                ; convert most sig digit
040D: 54 0F           anl   a,  #0fh         ; look at least sig half of acc
040F: 24 F6           add   a,  #0f6h        ; adjust it
0411: 50 02           jnc   noadj2           ; if a-f then re-adjust
0413: 24 07           add   a,  #07h
