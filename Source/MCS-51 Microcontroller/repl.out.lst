03D4: D0 82           pop   dpl
                   prtstr:
03D6: E4              clr  a                 ; set offset = 0
03D7: 93              movc a,  @a+dptr       ; get chr from code memory
03D8: B4 00 02        cjne a,  #0h, mchrok   ; if termination chr, then return
03DB: 80 06           sjmp prtdone
                   mchrok:
03DD: 12 03 B0        lcall sndchr           ; send character
03E0: A3              inc   dptr             ; point at next character
03E1: 80 F3           sjmp  prtstr           ; loop till end of string
                   prtdone:
03E3: 74 01           mov   a,  #1h          ; point to instruction after strin
03E5: 73              jmp   @a+dptr          ; return
